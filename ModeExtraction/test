string:add=("+")
string:sub=("-")
string:mul=("*")
string:div=("/")
string:mod=("%")
repeate:incre=($add {2,2})
repeate:reduc=($sub {2,2})
table:arithmetic=($add|$sub|$mul|$div|$mod|$incre|$reduc)

string:equal=("==")
string:noequal=("!=")
string:more=(">")
string:less=("<")
string:moe=(">=")
string:loe=("<=")
table:relation=($equal|$noequal|$more|$less|$moe|$loe)

string:and=("&")
string:or=("|")
string:no=("!")
regex:assignment=(/=|+=|-=|*=|/=/)
table:op=($arithmetic|$relation|$assignment)

string:public=("public")
string:private=("private")
string:protected=("protected")
table:modifier=($public|$private|protected)

string:null=("NULL")
string:true=("true")
string:false=("false")
string:class=("class")
string:interface=("interface")
string:abstract=("abstract")
string:extends=("extends")
string:new=("new")
string:const=("const")
string:goto=("goto")
string:package=("package")

string:byte=("byte")
string:char=("char")
string:boolean=("boolean")
string:short=("short")
string:int=("int")
string:float=("float")
string:long=("long")
string:double=("double")
string:void=("void")
table:datatype=($byte|$char|$boolean|$short|$int|$float|$long|$double|$void)

string:if=("if")
string:else=("else")
string:while=("while")
string:for=("for")
string:switch=("switch")
string:case=("case")
string:default=("default")
string:do=("do")
string:break=("break")
string:continue=("continue")
string:return=("return")
string:instanceof=("instanceof")
table:flow=($if|$else|$while|$for|$switch|$case|$default|$do|$break|$continue|$return|$instanceof)

string:static=("static")
string:final=("final")
string:super=("super")
string:this=("this")
string:native=("native")
string:strictfp=("strictfp")
string:synchronized=("synchronized")
string:transient=("transient")
string:volatile=("volatile")
table:modification=($static|$final|$super|$this|$native|$strictfp|$synchronized|$transient|$volatile)
string:enum=("enum")
string:assert=("assert")
string:catch=("catch")
string:try=("try")
string:finally=("finally")
string:throw=("throw")
string:throws=("throws")
table:exception=($catch|$try|$finally|$throw|$throws)
table:keyword= ($modifier|$datatype|$flow|$modification|$exception)

regex:start=(/+\s*/)
string:intent=("Intent")
regex:obj=(/[^\.\(\)=]\w+/)
regex:equal=(/\s*=\s*/)
regex:Intent_init =(/new\s+Intent\s*\((.*)\)\s*;/)

regex:add=(/\s*\+\s*/)
regex:sub=(/\s*-\s*/)
sequence:Intent_obj=($intent,$obj)
sequence:Intent_intent=($Intent_obj,$equal,$Intent_init)

sequence:add_Intent_intent=($add,$Intent_intent)
sequence:sub_Intent_intent=($sub,$Intent_intent)
regex:anything=(/[\s\S]/)

sequence:change_Intent_intent1=($add_Intent_intent,$$,$sub_Intent_intent)
sequence:change_Intent_intent2=($sub_Intent_intent,$$,$add_Intent_intent)
table:change_Intent_intent=( $change_Intent_intent1|$change_Intent_intent2)